local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Gamer-MouyuseV6/Xrypto-Lib/main/Source.lua"))()

local Config = Library:CreateConfig({
    Enable = true,
    Folder = "Hub",
    fileName = "Config"
})

local Window = Library:CreateLoader({
    ToggleKeybind = "RightControl",
})

Window.Scheme = {
    Backgroundcolor = "Rainbow",
    OutlineColor = "Purple"
}
