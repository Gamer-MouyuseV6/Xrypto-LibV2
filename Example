local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Gamer-MouyuseV6/Xrypto-Lib/refs/heads/main/Source.lua"))()

local Window = Library.CreateLoader({
    ToggleKeybind = "RightControl",
    UIColor = {
        MainUI = Color3.fromRGB(255, 100, 100),
        OutLineUI = Color3.fromRGB(255, 100, 100)
    }
})

Window:Visible(true)
