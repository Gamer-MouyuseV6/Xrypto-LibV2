local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Gamer-MouyuseV6/Xrypto-Lib/main/Source.lua"))()

local Config = Library:CreateConfig({
    Enable = true,
    Folder = "Hub",
    fileName = "Config"
})

local Key = Library:CreateLoader({
    Enable = true,
    Tittle = "Frog Hub",
    Tittle2 = "Key system",
    Key = {"Key"},
    SaveKey = true,
    AutoCheckIfKeyiscorrect = true,
    ToggleKeybind = "RightControl",
    SecondAction = {
        GetLinkType = "Link",
        Link = "https://discord.com/invite/abc123", -- Discord link (button will be blue)
        Button = GetLinkFrame,
        KeyBox = KeyBox,
        Callback = function()
            print("Key is correct!")
        end
    }
})
